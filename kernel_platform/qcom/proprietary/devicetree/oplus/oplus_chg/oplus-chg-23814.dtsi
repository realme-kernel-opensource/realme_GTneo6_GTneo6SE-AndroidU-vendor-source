			0 600  600  600  600  600  600  0	/* OPLUS_WIRED_CHG_MODE_SDP */
			0 1385 1600 1600 1600 1600 1600 0	/* OPLUS_WIRED_CHG_MODE_CDP */
			0 1385 2200 2200 2200 2200 2200 0	/* OPLUS_WIRED_CHG_MODE_DCP */
